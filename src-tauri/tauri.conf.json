{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "center": true,
        "decorations": true,
        "fullscreen": false,
        "height": 800,
        "minHeight": 600,
        "minWidth": 800,
        "resizable": true,
        "title": "Bibliotheca Vitae",
        "titleBarStyle": "Overlay",
        "transparent": true,
        "width": 1200
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "com.bibliotheca-vitae.app",
  "productName": "Digital-Garden",
  "version": "0.1.0"
}